{
  "name": "pix-bot",
  "version": "0.16.0",
  "description": "Pix Bot application",
  "main": "index.js",
  "scripts": {
    "configure": "cp sample.env .env && npm ci",
    "deploy": "npm run deploy:minor",
    "deploy:major": "npm version major && npm run deploy:tag",
    "deploy:minor": "npm version minor && npm run deploy:tag",
    "deploy:patch": "npm version patch && npm run deploy:tag",
    "deploy:tag": "git push --tags",
    "start": "node bin/www",
    "test": "NODE_ENV=test mocha --recursive --exit test",
    "lint": "eslint lib/* test/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1024pix/pix-bot.git"
  },
  "author": "GIP Pix",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/1024pix/pix-bot/issues"
  },
  "homepage": "https://github.com/1024pix/pix-bot#readme",
  "dependencies": {
    "@hapi/hapi": "^19.1.1",
    "@octokit/core": "^3.1.1",
    "@octokit/rest": "18.0.5",
    "@sentry/cli": "^1.55.0",
    "axios": "^0.21.1",
    "cron": "^1.8.2",
    "crypto": "^1.0.1",
    "dayjs": "^1.9.4",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "proxyquire": "^2.1.3",
    "scalingo": "^0.1.1",
    "scalingo-review-app-manager": "^1.0.0",
    "sib-api-v3-sdk": "^8.0.1",
    "tsscmp": "^1.0.6"
  },
  "engines": {
    "node": "14.15.1",
    "npm": "6.14.8"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^7.16.0",
    "mocha": "^8.2.1",
    "nock": "^13.0.5",
    "sinon": "^9.2.2"
  }
}
